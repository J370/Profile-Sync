{
    "name": "Profile Image Sync",
    "description": "Sync images from Whatsapp",
    "version": "1.5.0",
    "manifest_version": 2,
    "icons": {
        "16": "img/PIS-16.png",
        "48": "img/PIS-48.png",
        "128": "img/PIS-128.png"
    },
    "permissions": ["identity", "tabs", "storage", "*://web.whatsapp.com/", "*://people.googleapis.com/*"],
    "browser_action": {
        "default_popup": "index.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "key": "",
    "oauth2": {
        "client_id": "862546957628-vjdcj1r7f82uk26allsqvm109sci4vs4.apps.googleusercontent.com",
        "scopes": [
            "https://www.googleapis.com/auth/contacts"
        ]
    }
}